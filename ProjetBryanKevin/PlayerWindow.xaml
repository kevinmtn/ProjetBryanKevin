<Window x:Class="ProjetBryanKevin.PlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetBryanKevin"
        mc:Ignorable="d"
        Title="PlayerWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top" Background="#FF86726B">
                <MenuItem Header="_Deconnexion" Click="menuDisconnect" Foreground="White">
                </MenuItem>
                <MenuItem Header="_Options" Foreground="White">
                    <MenuItem Header="_Accueil" Click="menuHome"/>

                </MenuItem>
            </Menu>
        </DockPanel>
        <Frame Name="Main" Margin="0,20,0,0" NavigationUIVisibility="Hidden" Background="#FFBFBFBF" Navigated="Main_Navigated"/>
    </Grid>
</Window>
