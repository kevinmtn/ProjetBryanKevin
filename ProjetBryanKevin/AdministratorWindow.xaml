<Window x:Class="ProjetBryanKevin.AdministratorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetBryanKevin"
        mc:Ignorable="d"
        Title="AdministratorWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top" Background="#FF86726B" Height="Auto" Width="Auto">
                <MenuItem Header="_Accueil" Click="MenuHome" Foreground="White" Height="32" FontSize="13"/>
                <MenuItem Header="_Voir les jeux disponibles" Click ="MenuVideoGame" Foreground="White" Height="32" FontSize="13"/>
                <MenuItem Header="_Se déconnecter" Click="MenuDisconnect" Foreground="Red" Height="32" FontSize="13" HorizontalAlignment="Right" Width="107" HorizontalContentAlignment="Right"/>
            </Menu>

        </DockPanel>
        <Frame Name="Main" Margin="0,33,0,0" NavigationUIVisibility="Hidden" Background="#FFBFBFBF" />
    </Grid>
</Window>
